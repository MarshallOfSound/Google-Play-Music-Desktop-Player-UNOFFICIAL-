{
  "name": "google-play-music-desktop-player",
  "productName": "Google Play Music Desktop Player",
  "version": "3.4.0",
  "apiVersion": "1.0.0",
  "description": "An electron wrapper for Google Play Music",
  "main": "build/index.js",
  "scripts": {
    "build": "gulp build",
    "clean": "gulp clean",
    "lint": "eslint ./src/ ./test/ ./vendor/ gulpfile.babel.js",
    "make:darwin": "gulp make:darwin",
    "make:linux": "gulp make:linux",
    "make:deb": "gulp make:linux:deb",
    "make:rpm": "gulp make:linux:rpm",
    "make:deb:32": "gulp deb:linux:32",
    "make:deb:64": "gulp deb:linux:64",
    "make:rpm:32": "gulp rpm:linux:32",
    "make:rpm:64": "gulp rpm:linux:64",
    "make:win": "gulp make:win",
    "package:darwin": "gulp package:darwin",
    "package:linux": "gulp package:linux",
    "package:linux:32": "gulp package:linux:32",
    "package:linux:64": "gulp package:linux:64",
    "package:win": "gulp package:win",
    "postinstall": "node vendor/rebuild.js --instant",
    "test": "npm run lint",
    "pretest-unit": "npm run build",
    "pretest-spec": "npm run build",
    "test-unit": "electron-mocha ./test/electron --recursive --compilers js:babel-core/register --timeout 10000 -R spec-xunit-file",
    "test-spec": "mocha ./test/spectron --compilers js:babel-core/register -R spec-xunit-file --timeout 10000",
    "prestart": "pre-flight && node vendor/mac_patch_dev.js",
    "start": "electron . --dev",
    "watch": "gulp watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/MarshallOfSound/Google-Play-Music-Desktop-Player-UNOFFICIAL-"
  },
  "author": {
    "name": "Samuel Attard",
    "email": "samuel.r.attard@gmail.com",
    "url": "https://www.samuelattard.com"
  },
  "homepage": "http://www.googleplaymusicdesktopplayer.com",
  "license": "MIT",
  "dependencies": {
    "auto-launch-gpmdp": "^2.0.1",
    "electron-chromecast": "^1.0.14",
    "electron": "1.3.3",
    "html-entities": "^1.2.0",
    "gmusic-mini-player.js": "^2.0.3",
    "gmusic-theme.js": "^1.1.9",
    "gmusic-ui.js": "^2.0.2",
    "gmusic.js": "^6.0.7",
    "lastfm": "^0.9.2",
    "lodash": "^4.12.0",
    "mdns": "^2.3.3",
    "mkdirp": "^0.5.1",
    "node-fetch": "^1.5.2",
    "pretty-colorwheel": "^1.0.1",
    "request": "^2.72.0",
    "runas": "^3.1.1",
    "universal-analytics": "^0.4.2",
    "uuid": "^2.0.2",
    "winston": "^2.2.0",
    "ws": "^1.1.0",
    "xss": "^0.2.13",
    "yargs": "^4.7.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-fs": "chaijs/chai-fs",
    "devtron": "^1.0.1",
    "electron-mocha": "^3.0.0",
    "electron-packager": "^7.0.1",
    "electron-rebuild": "^1.1.4",
    "electron-winstaller": "2.2.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb-base": "^5.0.0",
    "eslint-plugin-import": "^1.7.0",
    "fs-extra": "^0.30.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.0",
    "gulp-cssmin": "^0.1.7",
    "gulp-header": "^1.8.2",
    "gulp-less": "^3.1.0",
    "gulp-replace": "^0.5.4",
    "gulp-uglify": "^2.0.0",
    "jquery": "^2.2.3",
    "material-design-icons-iconfont": "^2.0.5",
    "materialize-css": "^0.97.6",
    "mocha": "^3.0.0",
    "mocha-testdata": "^1.2.0",
    "plist": "^1.2.0",
    "pre-commit": "^1.1.2",
    "pre-flight": "^1.0.2",
    "request": "^2.72.0",
    "run-sequence": "^1.1.5",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "spec-xunit-file": "0.0.1-3",
    "spectron": "^3.2.3"
  },
  "optionalDependencies": {
    "appdmg": "^0.4.5",
    "mouse-forward-back": "^1.0.1",
    "dbus": "GPMDP/node-dbus#linux-only",
    "mpris-service": "GPMDP/mpris-service",
    "electron-installer-debian": "^0.3.0",
    "electron-installer-redhat": "^0.2.0",
    "ll-keyboard-hook-win": "^3.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
