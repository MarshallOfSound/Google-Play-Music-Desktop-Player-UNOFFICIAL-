{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/*.js": {
            "when": "$(basename).ts"
        },
        "**/*.*js": {
            "when": "$(basename).tsx"
        },
        "**/*.js.map": true,
        ".build/**/*": true,
        "build/**/*": true,
        "dist/**/*": true   
    }
}